    dcterms:title "Linking secondary metabolites to gene clusters through genome sequencing of six diverse Aspergillus species"@en ;
    prism:keyword "Aspergillus"@en, "comparative genomics"@en, "fumigatus"@en, "secondary metabolism."@en ;
    cito:extends sub:citingOrganism ;
