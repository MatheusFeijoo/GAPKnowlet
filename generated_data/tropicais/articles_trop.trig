    foaf:topic_interest "Comparative Study"@en ;
    dcterms:language dbpedia:English_language ;
    dcterms:title "Variation in secondary metabolite production potential in the Fusarium incarnatum-equiseti species complex revealed by comparative analysis of 13 genomes"@en ;
    prism:keyword "Comparative genome analyses"@en, "Fusarium incarnatum-equiseti species complex"@en, "Horizontal gene transfer"@en, "Phylogeny"@en, "Secondary metabolite genes."@en ;
    cito:extends sub:citingOrganism ;
