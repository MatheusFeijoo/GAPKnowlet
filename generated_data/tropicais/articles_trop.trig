    dcterms:title "Variability of the Pr77 sequence of L1Tc retrotransposon among six T. cruzi strains belonging to different discrete typing units (DTUs)"@en ;
    prism:keyword "Chagas disease"@en, "Discrete Type Units"@en, "Pr77-hallmark"@en, "Trypanosoma cruzi"@en, "genomic variability"@en, "retroelements."@en ;
    cito:extends sub:citingOrganism ;
    datacite:hasDescription sub:abstract .

  sub:paper_pmid a datacite:PrimaryResourceIdentifier ;
    prov:value <https://pubmed.ncbi.nlm.nih.gov/34273311/> ;
