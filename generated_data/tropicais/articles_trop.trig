    dcterms:title "Fusarium subtropicale, sp. nov., a novel nivalenol mycotoxin-producing species isolated from barley (Hordeum vulgare) in Brazil and sister to F. praegraminearum"@en ;
    prism:keyword "1 new taxon"@en, "Fusarium head blight"@en, "genotyping"@en, "morphology"@en, "pathogenicity"@en, "phylogenetics"@en, "trichothecene"@en, "whole genome."@en ;
    cito:extends sub:citingOrganism ;
    datacite:hasDescription sub:abstract .

  sub:paper_pmid a datacite:PrimaryResourceIdentifier ;
    prov:value <https://pubmed.ncbi.nlm.nih.gov/30303468/> ;
