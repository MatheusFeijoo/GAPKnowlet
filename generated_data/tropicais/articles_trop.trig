    dcterms:title "Improved genomic resources and new bioinformatic workflow for the carcinogenic parasite Clonorchis sinensis: Biotechnological implications"@en ;
    prism:keyword "Bioinformatics"@en, "Carcinogenic worm"@en, "Clonorchis sinensis"@en, "Comparative genomics."@en ;
    cito:extends sub:citingOrganism ;
