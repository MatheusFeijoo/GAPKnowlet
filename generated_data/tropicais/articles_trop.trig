    dcterms:title "Aspergillus flavus NRRL 35739, a Poor Biocontrol Agent, May Have Increased Relative Expression of Stress Response Genes"@en ;
    prism:keyword "Aspergillus flavus"@en, "abiotic stress"@en, "aflatoxin biocontrol"@en, "comparative transcriptomics."@en ;
    cito:extends sub:citingOrganism ;
