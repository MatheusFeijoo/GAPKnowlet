    dcterms:language dbpedia:English_language ;
    dcterms:title "Metabolic Features of Ganjang (a Korean Traditional Soy Sauce) Fermentation Revealed by Genome-Centered Metatranscriptomics"@en ;
    prism:keyword "fermentation"@en, "ganjang"@en, "genome centered"@en, "metabolic pathway"@en, "metagenome"@en, "metatranscriptome"@en, "soy sauce."@en ;
    cito:extends sub:citingOrganism ;
