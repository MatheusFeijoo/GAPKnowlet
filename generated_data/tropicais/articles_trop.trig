    dcterms:title "Comparative genomics reveals mobile pathogenicity chromosomes in Fusarium"@en ;
    cito:extends sub:citingOrganism ;
    datacite:hasDescription sub:abstract .

  sub:paper_pmid a datacite:PrimaryResourceIdentifier ;
    prov:value <https://pubmed.ncbi.nlm.nih.gov/20237561/> ;
