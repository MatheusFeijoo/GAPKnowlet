    dcterms:title "Draft genome sequence and chemical profiling of Fusarium langsethiae, an emerging producer of type A trichothecenes"@en ;
    prism:keyword "Cereals"@en, "Fusarium langsethiae"@en, "Genome"@en, "Glucosylated mycotoxins"@en, "HT-2 toxin"@en, "Oats"@en, "Secondary metabolism"@en, "T-2 toxin."@en ;
    cito:extends sub:citingOrganism ;
    datacite:hasDescription sub:abstract .

  sub:paper_pmid a datacite:PrimaryResourceIdentifier ;
