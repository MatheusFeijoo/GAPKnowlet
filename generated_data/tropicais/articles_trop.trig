    prism:keyword "Eurotiomycetes"@en, "allopolyploidy"@en, "ascomycota"@en, "aspergillosis"@en, "cryptic species"@en, "fungal pathogen evolution"@en, "hybridization"@en, "nonvertical evolution"@en, "pathogenicity"@en, "virulence."@en ;
    cito:extends sub:citingOrganism ;
