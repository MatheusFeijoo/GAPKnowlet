    dcterms:language dbpedia:English_language ;
    dcterms:title "Comparative genome analysis of Aspergillus flavus clinically isolated in Japan"@en ;
    prism:keyword "Aspergillus flavus"@en, "aflatoxin production"@en, "gene cluster for secondary metabolites."@en ;
    cito:extends sub:citingOrganism ;
    datacite:hasDescription sub:abstract .

  sub:paper_pmid a datacite:PrimaryResourceIdentifier ;
    prov:value <https://pubmed.ncbi.nlm.nih.gov/30520983/> ;
    datacite:usesIdentifierScheme edam:data_1187 .

  sub:paper_doi a datacite:PrimaryResourceIdentifier ;
    prov:value <https://doi.org/10.1128/genomea.00168-15> ;
