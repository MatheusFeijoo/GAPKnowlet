    dcterms:title "Comprehensive chemotaxonomic and genomic profiling of a biosynthetically talented Australian fungus, Aspergillus burnettii sp. nov"@en ;
    prism:keyword "Aspergillus"@en, "Biosynthetic gene cluster analysis"@en, "Carbohydrate active enzymes"@en, "Chemotaxonomy"@en, "Molecular phylogeny"@en, "Secondary metabolites."@en ;
    cito:extends sub:citingOrganism ;
