    dcterms:title "Comparative genomics of citric-acid-producing Aspergillus niger ATCC 1015 versus enzyme-producing CBS 513.88"@en ;
    cito:extends sub:citingOrganism ;
    datacite:hasDescription sub:abstract .

  sub:paper_pmid a datacite:PrimaryResourceIdentifier ;
    prov:value <https://pubmed.ncbi.nlm.nih.gov/21543515/> ;
