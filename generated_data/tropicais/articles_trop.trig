sub:assertion {
  this: a fabio:journalArticle ;
    datacite:hasIdentifier sub:paper_doi , sub:paper_pmcid ,  sub:paper_pmid ;
    dcterms:language dbpedia:English_language ;
    dcterms:title "Genomic and Phenotypic Analysis of COVID-19-Associated Pulmonary Aspergillosis Isolates of Aspergillus fumigatus"@en ;
    prism:keyword "Aspergillus"@en, "COVID-19"@en, "acute respiratory distress syndrome"@en, "aspergillosis"@en, "coinfection"@en, "pathogenicity"@en, "secondary infection"@en, "superinfection"@en, "virulence factors."@en ;
    cito:extends sub:citingOrganism ;
